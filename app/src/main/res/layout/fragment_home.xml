<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".home.HomeFragment">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <RelativeLayout
            android:id="@+id/rl_home"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_200sdp">

            <ImageView
                android:id="@+id/img_home"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_200sdp"
                android:src="@drawable/home"
                android:scaleType="fitXY"/>

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/half_transparent">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:paddingTop="@dimen/_4sdp"
                    android:paddingBottom="@dimen/_4sdp"
                    android:paddingStart="@dimen/_2sdp"
                    android:paddingEnd="@dimen/_2sdp"
                    android:paddingRight="@dimen/_2sdp"
                    android:background="@color/transparent"
                    android:layout_gravity="bottom"
                    android:paddingLeft="@dimen/_2sdp">

                    <ImageView
                        android:id="@+id/img"
                        android:layout_width="@dimen/_30sdp"
                        android:layout_height="@dimen/_30sdp"
                        android:layout_marginTop="@dimen/_4sdp"
                        android:layout_marginStart="@dimen/_8sdp"
                        android:layout_marginBottom="@dimen/_4sdp"
                        android:src="@drawable/ic_home_black_24dp"
                        android:layout_marginLeft="@dimen/_8sdp" />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textColor="@color/colorWhite"
                        android:layout_centerVertical="true"
                        android:textStyle="bold"
                        android:textSize="@dimen/_16sdp"
                        android:layout_marginStart="@dimen/_16sdp"
                        android:layout_marginEnd="@dimen/_16sdp"
                        android:text="Home"
                        android:layout_toEndOf="@+id/img"
                        android:layout_toRightOf="@+id/img" />
                </RelativeLayout>
            </FrameLayout>
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/rl_course"
            android:layout_below="@+id/rl_home"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_200sdp">

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:src="@drawable/course"
                android:scaleType="fitXY"/>

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/half_transparent">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:paddingTop="@dimen/_4sdp"
                    android:paddingBottom="@dimen/_4sdp"
                    android:paddingStart="@dimen/_2sdp"
                    android:paddingEnd="@dimen/_2sdp"
                    android:paddingRight="@dimen/_2sdp"
                    android:background="@color/transparent"
                    android:layout_gravity="bottom"
                    android:paddingLeft="@dimen/_2sdp">

                    <ImageView
                        android:id="@+id/img_course"
                        android:layout_width="@dimen/_30sdp"
                        android:layout_height="@dimen/_30sdp"
                        android:layout_marginTop="@dimen/_4sdp"
                        android:layout_marginStart="@dimen/_8sdp"
                        android:layout_marginBottom="@dimen/_4sdp"
                        android:src="@drawable/ic_home_black_24dp"
                        android:layout_marginLeft="@dimen/_8sdp" />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textColor="@color/colorWhite"
                        android:layout_centerVertical="true"
                        android:textStyle="bold"
                        android:textSize="@dimen/_16sdp"
                        android:layout_marginStart="@dimen/_16sdp"
                        android:layout_marginEnd="@dimen/_16sdp"
                        android:text="Course"
                        android:layout_toEndOf="@+id/img_course"
                        android:layout_toRightOf="@+id/img_course" />
                </RelativeLayout>
            </FrameLayout>







        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/service"
            android:layout_below="@+id/rl_course"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_200sdp">

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:src="@drawable/service"
                android:scaleType="fitXY"/>

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/half_transparent">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:paddingTop="@dimen/_4sdp"
                    android:paddingBottom="@dimen/_4sdp"
                    android:paddingStart="@dimen/_2sdp"
                    android:paddingEnd="@dimen/_2sdp"
                    android:paddingRight="@dimen/_2sdp"
                    android:background="@color/transparent"
                    android:layout_gravity="bottom"
                    android:paddingLeft="@dimen/_2sdp">

                    <ImageView
                        android:id="@+id/img_service"
                        android:layout_width="@dimen/_30sdp"
                        android:layout_height="@dimen/_30sdp"
                        android:layout_marginTop="@dimen/_4sdp"
                        android:layout_marginStart="@dimen/_8sdp"
                        android:layout_marginBottom="@dimen/_4sdp"
                        android:src="@drawable/ic_home_black_24dp"
                        android:layout_marginLeft="@dimen/_8sdp" />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textColor="@color/colorWhite"
                        android:layout_centerVertical="true"
                        android:textStyle="bold"
                        android:textSize="@dimen/_16sdp"
                        android:layout_marginStart="@dimen/_16sdp"
                        android:layout_marginEnd="@dimen/_16sdp"
                        android:text="Service"
                        android:layout_toEndOf="@+id/img_service"
                        android:layout_toRightOf="@+id/img_service" />
                </RelativeLayout>
            </FrameLayout>







        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/rl_about"
            android:layout_below="@+id/service"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_200sdp">

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:src="@drawable/about"
                android:scaleType="fitXY"/>

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/half_transparent">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:paddingTop="@dimen/_4sdp"
                    android:paddingBottom="@dimen/_4sdp"
                    android:paddingStart="@dimen/_2sdp"
                    android:paddingEnd="@dimen/_2sdp"
                    android:paddingRight="@dimen/_2sdp"
                    android:background="@color/transparent"
                    android:layout_gravity="bottom"
                    android:paddingLeft="@dimen/_2sdp">

                    <ImageView
                        android:id="@+id/img_about"
                        android:layout_width="@dimen/_30sdp"
                        android:layout_height="@dimen/_30sdp"
                        android:layout_marginTop="@dimen/_4sdp"
                        android:layout_marginStart="@dimen/_8sdp"
                        android:layout_marginBottom="@dimen/_4sdp"
                        android:src="@drawable/ic_home_black_24dp"
                        android:layout_marginLeft="@dimen/_8sdp" />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textColor="@color/colorWhite"
                        android:layout_centerVertical="true"
                        android:textStyle="bold"
                        android:textSize="@dimen/_16sdp"
                        android:layout_marginStart="@dimen/_16sdp"
                        android:layout_marginEnd="@dimen/_16sdp"
                        android:text="About"
                        android:layout_toEndOf="@+id/img_about"
                        android:layout_toRightOf="@+id/img_about" />
                </RelativeLayout>
            </FrameLayout>







        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/rl_contact"
            android:layout_below="@+id/service"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_200sdp">

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:src="@drawable/contact"
                android:scaleType="fitXY"/>

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/half_transparent">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:paddingTop="@dimen/_4sdp"
                    android:paddingBottom="@dimen/_4sdp"
                    android:paddingStart="@dimen/_2sdp"
                    android:paddingEnd="@dimen/_2sdp"
                    android:paddingRight="@dimen/_2sdp"
                    android:background="@color/transparent"
                    android:layout_gravity="bottom"
                    android:paddingLeft="@dimen/_2sdp">

                    <ImageView
                        android:id="@+id/img_contact"
                        android:layout_width="@dimen/_30sdp"
                        android:layout_height="@dimen/_30sdp"
                        android:layout_marginTop="@dimen/_4sdp"
                        android:layout_marginStart="@dimen/_8sdp"
                        android:layout_marginBottom="@dimen/_4sdp"
                        android:src="@drawable/ic_home_black_24dp"
                        android:layout_marginLeft="@dimen/_8sdp" />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textColor="@color/colorWhite"
                        android:layout_centerVertical="true"
                        android:textStyle="bold"
                        android:textSize="@dimen/_16sdp"
                        android:layout_marginStart="@dimen/_16sdp"
                        android:layout_marginEnd="@dimen/_16sdp"
                        android:text="Contact"
                        android:layout_toEndOf="@+id/img_contact"
                        android:layout_toRightOf="@+id/img_contact" />
                </RelativeLayout>
            </FrameLayout>







        </RelativeLayout>
    </RelativeLayout>



</android.support.v4.widget.NestedScrollView>

